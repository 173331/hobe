if Config.AutoScan then
    wait(1)
    print("[CHAOS] Auto scanning...")
    Scanner:FullScan()
    print("[CHAOS] Found " .. Scanner.Stats.TotalScanned .. " remotes")
    print("[CHAOS] High: " .. Scanner.Stats.HighRisk .. " | Medium: " .. Scanner.Stats.MediumRisk .. " | Low: " .. Scanner.Stats.LowRisk)
end

