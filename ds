-- Create UI
local MainUI = UI:Create()

-- Auto scan on load
if Config.AutoScan then
    task.wait(1)
    print("[CHAOS SCANNER] Starting auto scan...")
    Scanner:FullScan()
    print("[CHAOS SCANNER] Found " .. Scanner.Stats.TotalScanned .. " remotes")
    print("[CHAOS SCANNER] High Risk: " .. Scanner.Stats.HighRisk)
    print("[CHAOS SCANNER] Medium Risk: " .. Scanner.Stats.MediumRisk)
    print("[CHAOS SCANNER] Low Risk: " .. Scanner.Stats.LowRisk)
end

print("[CHAOS SCANNER] Ready! Press SCAN button to start.")
